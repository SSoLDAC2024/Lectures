PREFIX bot: <https://w3id.org/bot#>
PREFIX spatial: <http://example.org/spatial#>
PREFIX inst: <http://example.org/instances#>

inst:cube_1 a bot:element ;
    spatial:hasX 1 ;
    spatial:hasY 1 ;
    spatial:hasZ 1 .

inst:cube_2 a bot:element ;
    spatial:hasParent inst:cube_1 ;
    spatial:hasX 1 ;
    spatial:hasY 1 ;
    spatial:hasZ 1 .

inst:cube_21 a bot:element ;
    spatial:hasParent inst:cube_1 ;
    spatial:hasX 2 ;
    spatial:hasY 2 ;
    spatial:hasZ 2 .

inst:cube_221 a bot:element ;
    spatial:hasParent inst:cube_21 ;
    spatial:hasX 1 ;
    spatial:hasY 1 ;
    spatial:hasZ 1 .

inst:cube_3 a bot:element ;
    spatial:hasParent inst:cube_2 ;
    spatial:hasX 1 ;
    spatial:hasY 1 ;
    spatial:hasZ 1 .

inst:cube_4 a bot:element ;
    spatial:hasParent inst:cube_3 ;
    spatial:hasX 1 ;
    spatial:hasY 1 ;
    spatial:hasZ 1 .

inst:cube_5 a bot:element ;
    spatial:hasParent inst:cube_4 ;
    spatial:hasX 1 ;
    spatial:hasY 1 ;
    spatial:hasZ 1 .